{

	"name": "Blocker",
	"version": "0.11",
	"manifest_version":2,
	"description": "Blocks Youtube & other distractions",
	"background":{
		"scripts": ["background.js", "back.js"],
		"persistant": false

	},
	"icons": {
		"128": "images/no.png"
	},
	"browser_action":{
		"default_icon": "images/no2.png",
		"default_title": "-Blocking-",
		"default_popup": "popup.html"
	},
	"permissions":[
		"webRequest",
		"webRequestBlocking",
		"alarms",
		"https://www.youtube.com/*",
		"https://google.com/*",
		"activeTab",
		"declarativeContent",
		"storage",
		"tabs", "<all_urls>"
	],
	"web_accessible_resources":[
	"media/chime.wav"
	]
//	"page_action": {
//       "default_popup": "popup.html"
//	}











}











